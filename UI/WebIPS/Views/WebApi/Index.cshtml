@model IEnumerable<string>
@{
    ViewData["Title"] = "WebAPI";
}

<div>
    <h1>@ViewData["Title"]</h1>
    <table>
        <tr>
            <th>#</th>
            <th>value</th>
        </tr>
        @{
            var i = 0;
        }
        @foreach (var value in Model)
        {
            <tr>
                <td>@(i++)</td>
                <td>@value</td>
            </tr>
        }
    </table>

</div>
